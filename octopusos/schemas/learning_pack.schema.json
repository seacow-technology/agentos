{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$version": "1.0.0",
  "title": "LearningPack",
  "type": "object",
  "required": [
    "source_runs",
    "pattern",
    "proposed_memory_items"
  ],
  "properties": {
    "source_runs": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "pattern": {
      "type": "string"
    },
    "proposed_memory_items": {
      "type": "array"
    },
    "proposed_policy_patch": {
      "type": "object"
    },
    "confidence": {
      "type": "number"
    },
    "verification_plan": {
      "type": "string"
    }
  }
}