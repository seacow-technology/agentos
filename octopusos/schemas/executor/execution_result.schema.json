{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "agentos://schemas/executor/execution_result.schema.json",
  "title": "Execution Result (v0.11.1)",
  "description": "Result of real execution with rollback points",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "execution_result_id",
    "schema_version",
    "execution_request_id",
    "status",
    "started_at",
    "completed_at"
  ],
  "properties": {
    "execution_result_id": {
      "type": "string",
      "pattern": "^exec_result_[a-z0-9_]{6,64}$"
    },
    "schema_version": {
      "type": "string",
      "const": "0.11.1"
    },
    "execution_request_id": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": ["success", "partial_success", "failed", "rolled_back"]
    },
    "operations_executed": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["operation_id", "type", "status"],
        "properties": {
          "operation_id": {"type": "string"},
          "type": {"type": "string"},
          "status": {"type": "string", "enum": ["success", "failed", "skipped"]},
          "files_modified": {"type": "array", "items": {"type": "string"}},
          "checksum": {"type": "string"}
        }
      }
    },
    "rollback_point": {
      "type": "object",
      "properties": {
        "commit_hash": {"type": "string"},
        "worktree_path": {"type": "string"}
      }
    },
    "started_at": {"type": "string", "format": "date-time"},
    "completed_at": {"type": "string", "format": "date-time"}
  }
}
