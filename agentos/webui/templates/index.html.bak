    <!-- Initialize Sentry (must be before other scripts) -->
    <script>
        // Sentry Configuration
        // Controlled by backend SENTRY_ENABLED environment variable
        const SENTRY_ENABLED = {{ 'true' if sentry_enabled else 'false' }};
        const SENTRY_DSN = "{{ sentry_dsn | default('') }}";
        const SENTRY_ENVIRONMENT = "{{ sentry_environment | default('development') }}";
        const SENTRY_RELEASE = "{{ sentry_release | default('agentos-webui@0.3.2') }}";

        if (SENTRY_ENABLED && typeof Sentry !== 'undefined') {
            Sentry.init({
                dsn: SENTRY_DSN,
                environment: SENTRY_ENVIRONMENT,
                release: SENTRY_RELEASE,

                // Performance Monitoring
                integrations: [
                    Sentry.browserTracingIntegration({
                        // Track page loads and navigation
                        tracePropagationTargets: ["localhost", /^\//],
                    }),
                    Sentry.replayIntegration({
                        // Session Replay for debugging
                        maskAllText: false,
                        blockAllMedia: false,
                    }),
                ],

                // Performance Monitoring: 100% in dev, adjust in production
                tracesSampleRate: 1.0,

                // Session Replay: 10% of sessions
                replaysSessionSampleRate: 0.1,
                // Session Replay: 100% of error sessions
                replaysOnErrorSampleRate: 1.0,

                // Auto Session Tracking (Release Health)
                autoSessionTracking: true,

                // Automatically capture unhandled promise rejections
                captureUnhandledRejections: true,

                // Filter out healthcheck noise
                beforeSend(event, hint) {
                    const url = event.request?.url || '';
                    // Skip health check endpoints
                    if (url.includes('/health') || url.includes('/api/health')) {
                        return null;
                    }
                    return event;
                },
            });

            console.log('check Sentry initialized:', SENTRY_ENVIRONMENT, SENTRY_RELEASE);

            // Set user context if available (can be updated dynamically)
            // Sentry.setUser({ id: "user_id", email: "user@example.com" });
        } else if (!SENTRY_ENABLED) {
            console.log('âŠ˜ Sentry monitoring disabled');
        } else {
            console.warn('warning Sentry SDK not loaded');
        }
    </script>

