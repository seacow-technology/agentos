// 导出类型用于类型推断
export type TextKey = typeof K[keyof typeof K][keyof typeof K[keyof typeof K]] | string

// 提取所有 leaf keys
export type AllTextKeys = ExtractLeafKeys<typeof K>

type ExtractLeafKeys<T> = T extends object
  ? {
      [P in keyof T]: T[P] extends string
        ? T[P]
        : ExtractLeafKeys<T[P]>
    }[keyof T]
  : never
